<!--edit_userの画面-->
<h1>users/:id/editの画面</h1>

<div class="container">
  <h1>User info</h1>
  <%= form_with model: @user do |f| %>
  <h4>name</h1>
  <%= f.text_field :name %>

  <h4>Image</h4>
  <h5>ファイルを選択</h5>

  <h4>Introduction</h4>
  <%= f.text_area :introduction %></br>
  <%= f.submit "Update User" %>
  <% end %>

</div>